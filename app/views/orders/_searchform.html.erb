<%= search_form_for @search, html: { class: "form-inline"}, remote: true do |f|  %>
  <div class="col-md-10">
      <%= f.text_field :schedule_date_cont, placeholder: "Find by date", class: "form-control", id: "searchdate", :'data-provide' => 'datepicker'%>
    <div class="form-group">
      <%= f.submit "Search", class: "btn btn-primary" %>
    </div>
  </div>
<% end %>
